{
  "name": "@belsrc/worker-promise",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "gitHooks": {
    "commit-msg": "commitlint -E GIT_PARAMS",
    "pre-commit": "npm run pre-commit-hook"
  },
  "scripts": {
    "lint": "npx eslint -c .eslintrc.js --fix --color --ignore-path .eslintignore \"src/**/*.js\"",
    "prettier": "npx prettier --config .prettierrc.js --write \"src/**/*.js\"",
    "clean": "npm-run-all prettier lint",
    "test": "jest --verbose",
    "test:changed": "jest -o",
    "test:cov": "jest --verbose --coverage",
    "coverage": "jest --verbose --coverage && codecov",
    "build": "npx babel src --out-dir dist --ignore **/*.test.js",
    "pre-commit-hook": "npm-run-all clean test:changed",
    "check": "npx npm-check -u"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belsrc/worker-promise.git"
  },
  "bugs": {
    "url": "https://github.com/belsrc/worker-promise/issues"
  },
  "homepage": "https://github.com/belsrc/worker-promise#readme",
  "author": "bryan@bryanckizer.com",
  "license": "MIT",
  "keywords": [
    "webworker",
    "web",
    "worker",
    "promise"
  ],
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "babel-eslint": "10.0.1",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "codecov": "3.5.0",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-belsrc": "1.0.1",
    "jest": "24.8.0",
    "npm-check": "5.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "yorkie": "2.0.0"
  }
}
